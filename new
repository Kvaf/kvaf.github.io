                <h3 className="text-2xl font-bold mb-6">Search Results for "{searchTerm}"</h3>
                <div className="grid gap-4">
                  {searchResults.map((domain, index) => (
                    <div key={index} className={`p-4 rounded-lg border-2 ${
                      domain.available 
